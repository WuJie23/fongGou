.imgbackgroundimage {
  display: block;
  position: relative;
}
.imgBoxBack {
  display: flex;
  position: absolute;
  top: 0;
  left: 0;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 480rpx;
  color: #fff;
  font-size: 36rpx;
  backdrop-filter: blur(18rpx);
}

.img {
  width: 180rpx !important;
  height: 180rpx !important;
  border-radius: 50%;
  margin-bottom: 10rpx;
  box-shadow: #949494 0 0 50rpx;
}
